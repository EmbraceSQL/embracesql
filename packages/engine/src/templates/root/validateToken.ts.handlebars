import * as types from "./index";

export const validateToken : types.SystemHandler = async (context: types.Context) => {
  // default action is to ask the root -- the core system and configuration
  // to look for and parse OpenID tokens 
  // you can of course change this to suit your needs
  await types.validateOpenIDAuthorization(context);
}